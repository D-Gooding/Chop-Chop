    {
      "id" : "1",
      "image" : "QuornSpagBol.png",
      "name": "Quorn Spaghetti Bolognese",
      "description": "It's like bolognese but veggie with spaghetti",
      "prepTime": "10 minutes",
      "cookTime": "25 minutes",
      "ingredients": [
        { "item": "Red Onion", "amount": "1", "unit": "unit" },
        { "item": "Carrots", "amount": "2", "unit": "unit" },
        { "item": "Chopped Tomatoes", "amount": "400", "unit": "g" },
        { "item": "Quorn Mince", "amount": "500", "unit": "g" },
        { "item": "Vegetable Stock Cube", "amount": "1", "unit": "unit" },
        { "item": "Spaghetti", "amount": "500", "unit": "g" }
      ],
      "steps": [
        {
          "command": "Get the red onion",
          "progressionObject": "Red-Onion",
          "inhibitor": "none",
          "camera": "prep"
        },
        {
          "command": "Chop the red onion",
          "progressionObject": "Chopped-Red-Onion",
          "inhibitor": "Red-Onion",
          "camera": "prep"
        },
        {
          "command": "Get the carrots",
          "progressionObject": "Carrot",
          "inhibitor": "none",
          "camera": "prep"
        },
        {
          "command": "Dice the carrots",
          "progressionObject": "Diced-Carrot",
          "inhibitor": "Carrot",
          "camera": "prep"
        },
        {
          "command": "Lightly oil the pan and add to heat",
          "progressionObject": "Empty-Pan",
          "inhibitor": "none",
          "camera": "cook"
        },
        {
          "command": "Add the chopped onions to the pan",
          "progressionObject": "Red-Onion-Carrots-Pan",
          "inhibitor": "Pan",
          "camera": "cook"
        },
        {
          "command": "Wait 4 minutes for onions to being to fry",
          "progressionObject": "timer",
          "inhibitor": "4",
          "camera": "cook"
        },
        {
          "command": "Add the diced carrots to the pan",
          "progressionObject": "timer",
          "inhibitor": "4",
          "camera": "cook"
        },
        {
          "command": "Wait 4 minutes for onions and carrots to fry together",
          "progressionObject": "timer",
          "inhibitor": "4",
          "camera": "cook"
        },
        {
          "command": "Add the Chopped Tomatoes to the fried onions and carrots",
          "progressionObject": "Onion-Carrot-Tomato-Pan",
          "inhibitor": "Red-Onion-Carrots-Pan",
          "camera": "cook"
        },
        {
          "command": "Add mixed herbs and seasoning to taste",
          "progressionObject": "Mixed-Herbs",
          "inhibitor": "none",
          "camera": "cook"
        },
        {
          "command": "Mix vegetable stock cube with boiling water and add to frying pan",
          "progressionObject": "timer",
          "inhibitor": "2",
          "camera": "cook"
        },
        {
          "command": "Add quorn mince to the pan with stock in",
          "progressionObject": "Quorn",
          "inhibitor": "Onion-Carrot-Tomato-Pan",
          "camera": "cook"
        },
        {
          "command": "Mix quorn mince into sauce and turn heat down to simmer",
          "progressionObject": "Bolognese-Pan",
          "inhibitor": "Quorn",
          "camera": "cook"
        },
        {
          "command": "Add water to saucepan and add to heat",
          "progressionObject": "Water-Saucepan",
          "inhibitor": "none",
          "camera": "cook"
        },
        {
          "command": "Wait for saucepan to begin to boil",
          "progressionObject": "Boiling-Water-Saucepan",
          "inhibitor": "Water-Saucepan",
          "camera": "cook"
        },
        {
          "command": "Add the spaghetti to the boiling saucepan",
          "progressionObject": "Raw-Pasta-Saucepan",
          "inhibitor": "Boiling-Water-Saucepan",
          "camera": "cook"
        },
        {
          "command": "Wait for the spaghetti to be cooked",
          "progressionObject": "timer",
          "inhibitor": "10",
          "camera": "cook"
        },
        {
          "command": "Drain spaghetti and serve topped with bolognese sauce",
          "progressionObject": "none",
          "inhibitor": "none",
          "camera": "none"
        }
      ]
    },
